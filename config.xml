<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.geolocation" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Cordova Base Plugin geolocation</name>
	<description>A sample Cordova Base Plugin geolocation</description>
	<author email="dev@cordova.apache.org" href="http://cordova.io">Cordova Base Plugin geolocation</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
			<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
</widget>
